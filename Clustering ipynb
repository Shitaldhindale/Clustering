    "#### Checking for all the values in dataset of type integer, if not replacing them with nan"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "943c238c",
   "metadata": {},
   "outputs": [],
   "source": [
    "def check_int(df):\n",
    "    count = 0\n",
    "    for row in df:\n",
    "        try:\n",
    "            if type(row) != int:\n",
    "                df.loc[count] = np.nan\n",
    "        except:\n",
    "            pass\n",
    "        count +=1"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "97a9b3c4",
   "metadata": {},
   "outputs": [],
   "source": [
    "check_int(data[data.columns])"
   ]
  },
